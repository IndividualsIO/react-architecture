{
  "name": "react-architecture",
  "version": "1.0.0",
  "descriptiocn": "A small project of architecture to avoid to start a project from the very beginning using React + Redux in a simple way.",
  "author": "Carlos Abdalla",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "start": "npm-run-all --parallel start-dash",
    "begin": "babel-node tools/server.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "start-dash": "webpack-dashboard -- babel-node tools/server.js",
    "start-message": "babel-node tools/startMessage.js",
    "test": "mocha --reporter progress tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "node_modules/.bin/esw webpack.config.* src tools --fix",
      "git add"
    ],
    "*.css": [
      "node_modules/.bin/prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "domkit": "^0.0.1",
    "history": "^4.7.2",
    "prop-types": "15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "redux-thunk": "^2.2.0",
    "toastr": "^2.1.4"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babylon": "^6.18.0",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.2.1",
    "compression": "^1.7.2",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-async-await": "^0.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^3.1.4",
    "expect": "^22.4.3",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "jsdom": "^11.7.0",
    "lint-staged": "^7.0.4",
    "mocha": "^5.0.5",
    "node-sass": "4.8.3",
    "npm-run-all": "^4.1.2",
    "open": "^0.0.5",
    "plop": "^2.0.0",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.3",
    "prettier": "1.11.1",
    "react-hot-loader": "4.0.1",
    "react-test-renderer": "16",
    "redux-immutable-state-invariant": "^2.1.0",
    "rimraf": "^2.6.2",
    "sass-loader": "6.0.7",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-dev-server": "^3.1.3",
    "webpack-hot-middleware": "^2.22.0"
  }
}
